color status cyan default

mailboxes =INBOX
set certificate_file=$HOME/.mutt/contact@jakubpawlowicz.com/certificates
set folder="imaps://mail.runbox.com:993/"
set from="contact@jakubpawlowicz.com"
set header_cache=$HOME/.mutt/contact@jakubpawlowicz.com/cache/headers
set imap_user="contact%jakubpawlowicz.com"
set message_cachedir=$HOME/.mutt/contact@jakubpawlowicz.com/cache/bodies
set postponed="+Drafts"
set realname="Jakub Pawlowicz"
set record="+Sent"
set smtp_url="smtp://contact%25jakubpawlowicz.com@mail.runbox.com:587/"
set spoolfile="+INBOX"

source "gpg2 -d $HOME/.mutt/contact@jakubpawlowicz.com/password.gpg |"

